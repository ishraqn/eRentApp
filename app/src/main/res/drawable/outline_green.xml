<?xml version="1.0" encoding="utf-8"?>
<!-- This Gradient Outline draw code was based off an online tutorial. -->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <gradient
                android:angle="270"
                android:endColor="@color/green_400"
                android:startColor="@color/green_600" />
            <corners android:radius="10dp" />
            <padding
                android:bottom="4dp"
                android:left="4dp"
                android:right="4dp"
                android:top="4dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <gradient
                android:startColor="@color/white"
                android:endColor="@color/lt_grey"
                android:type="linear"
                android:angle="120"/>
            <corners android:radius="10dp" />
            <size
                android:width="50dp"
                android:height="50dp" />
        </shape>
    </item>
</layer-list>